mod_ui_md5="`md5sum $MODPATH/module.prop | cut -d " " -f1`"
  echo "  MD5_1：$mod_ui_md5" 
  mod_official_ui_md5="`cat $MODPATH/system/vendor/etc/device_features/y`"
  if [ ! "$mod_ui_md5" = "$mod_official_ui_md5" ];then
  echo "  官方MD5_1：$mod_official_ui_md5"
    echo "******************************"
    echo "  检测到您的的MD5与官方不匹配"
    echo "******************************"
    echo "  请重新下载文件,或将您更改的数据还原"
    echo "  勿二改此模块谢谢"
    echo "  交流群:647299031"
    echo "******************************"
    abort
    md5_ui=true
  fi
  
  
  
  
  cus_ui_md5="`md5sum $MODPATH/customize.sh | cut -d " " -f1`"
  echo "  MD5_2：$cus_ui_md5" 
  cus_official_ui_md5="`cat $MODPATH/system/vendor/etc/device_features/e`"
  if [ ! "$cus_ui_md5" = "$cus_official_ui_md5" ];then
  echo "  官方MD5_2：$cus_official_ui_md5"
    echo "******************************"
    echo "  检测到您的的MD5_2与官方不匹配"
    echo "******************************"
    echo "  请重新下载文件,或将您更改的数据还原"
    echo "  勿二改此模块谢谢"
    echo "  交流群:647299031"
    echo "******************************"
    abort
    md5_ui=true
  fi
  
  
  


 